<Window x:Class="Natural_Graphics_Primitives_UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Natural_Graphics_Primitives_UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <TextBlock Grid.Row="0" Grid.Column="0" x:Name="TextBlock" Text="Natural Graphics Primitives UI" FontSize="24"/>
        <TextBlock Grid.Row="1" Grid.Column="0" Text="Copyright (c) 2018, Natural Graphics Primitives" FontSize="12"/>
        <Button Grid.Row="1" Grid.Column="1" Content="Start" Click="Start"/>
    </Grid>
</Window>
